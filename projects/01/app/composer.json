{
    "name": "atlance/otus-php-pro-01",
    "description": "Course of «PHP Developer. Professional» from OTUS. Homework.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Stepanov",
            "email": "lanposts@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-memcached": "^3.2",
        "ext-pdo": "*",
        "ext-redis": "^5.3",
        "ext-zip": "^1.19",
        "symfony/dotenv": "^6.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
